{
    "name": "PrimePlus",
    "type": "dark", // or "light"
    "colors": {
        "editor.background": "#1E1E1E",
        "editor.foreground": "#FFFFFF",
        "editorCursor.foreground": "#68B1FF",
        "editor.lineHighlightBackground": "#2A2A2A"
    },
    "tokenColors": [
        {
            "scope": ["comment"],
            "settings": {
                "foreground": "#FF0000"
            }
        },
        {
            "scope": ["keyword"],
            "settings": {
                "foreground": "#68B1FF"
            }
        },
        {
            "scope": ["strings"],
            "settings": {
                "foreground": "#FC635F"
            }
        },
        {
            "scope": ["markup"],
            "settings": {
                "foreground": "#FC635F"
            }
        },{
            "scope": ["punctuation.definition.parameters.hp-prime"],
            "settings": {
                "foreground": "#FC635F"
            }
        },
        {
            "scope": ["constant.numeric.hp-prime"],
            "settings": {
                "foreground": "#EBEBEB"
            }
        },
        {
            "scope": ["punctuation.definition.block.hp-prime"],
            "settings": {
                "foreground": "#FC635F"
            }
        },
        {
            "scope": ["meta.brace", "meta.brace.hp-prime"],
            "settings": {
                "foreground": "#FC635F"
            }
        },
        {
            "scope": ["meta.block.hp-prime", "meta.function.hp-prime"],
            "settings": {
                "foreground": "#FC635F"
            }
        }
    ]
}
